{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-config-prettier"
  ],
  "rulesDirectory": [
    "tslint-plugin-prettier"
  ],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "jsx-no-lambda": false,
    "no-shadowed-variable": false,
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "member-access": false,
    "no-console": false,
    "no-var-requires": false,
    "quotemark": [true, "single", "backtick"]
  }
}